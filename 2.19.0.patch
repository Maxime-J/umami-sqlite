+  dateValue   DateTime? @map("date_value")
+  dataType    Int       @map("data_type")
+  distinctId  String?   @map("distinct_id")
+  createdAt   DateTime? @default(dbgenerated("unixepoch() * 1000")) @map("created_at")
+
