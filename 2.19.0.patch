+  websiteEventId String    @map("website_event_id")
+  dataKey        String    @map("data_key")
+  stringValue    String?   @map("string_value")
+  numberValue    Decimal?  @map("number_value")
+  dateValue      DateTime? @map("date_value")
+  dataType       Int       @map("data_type")
